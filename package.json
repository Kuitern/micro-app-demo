{
  "name": "micro-demo-master",
  "version": "1.0.0",
  "description": "前端微MicroApp",
  "main": "index.js",
  "scripts": {
    "all:install": "npm-run-all --serial install:*",
    "all:start": "npm-run-all --parallel start:*",
    "install:base": "cd base && npm install",
    "install:app_first": "cd app_first && npm install",
    "install:app_second": "cd app_second && npm install",
    "start:base": "cd base && npm run serve",
    "start:app_first": "cd app_first && npm run serve",
    "start:app_second": "cd app_second && npm run serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
